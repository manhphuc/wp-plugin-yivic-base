{
  "name": "yivic/yivic-base",
  "type": "wordpress-muplugin",
  "description": "The base plugin to have powerful Laravel development on WordPress",
  "version": "v0.7.0",
  "time": "2024-05-23 20:14:04",
  "homepage": "https://yivic.com/wp-plugin-yivic-base",
  "license": "MIT",
  "authors": [
    {
      "name": "Phuc Nguyen",
      "email": "manhphucofficial@yahoo.com",
      "homepage": "https://yivic.com/members/phuc-nguyen",
      "role": "Developer"
    }
  ],
  "require": {
    "laravel/framework": "~8.83.0",
    "spatie/laravel-html": "~2.30.0",
    "laravel/tinker": "~2.9.0",
    "doctrine/dbal": "~3.3.8",
    "symfony/var-dumper": "~5.4.39",
    "mcaskill/composer-exclude-files": "~4.0.0",
    "php": "^7.3|^8.0"
  },
  "require-dev": {
    "automattic/vipwpcs": "~3.0.0",
    "phpcompatibility/php-compatibility": "~9.3.5",
    "dealerdirect/phpcodesniffer-composer-installer": "~1.0.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  ],
  "autoload": {
    "files": [
      "src/Foundation/Support/helpers-utils.php",
      "./yivic-base-bootstrap.php",
      "./yivic-base-init.php"
    ],
    "psr-4": {
      "Carbon\\Traits\\Creator\\": "src/Foundation/Support/Carbon/Traits/Creator.php",
      "Yivic_Base\\Deps\\": "src-deps",
      "Yivic_Base\\": "src"
    }
  },
  "autoload-dev": {
    "files": [
    ],
    "psr-4": {
      "Yivic_Base\\Tests\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.3.0"
    },
    "platform-check": false,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "mcaskill/composer-exclude-files": true,
      "composer/installers": true
    },
    "vendor-dir": "./vendor",
    "sort-packages": true
  },
  "extra": {
    "exclude-from-files": [
      "laravel/framework/src/Illuminate/Foundation/helpers.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"copy('src-override/nesbot/carbon/src/Carbon/Traits/Creator.php', 'vendor/nesbot/carbon/src/Carbon/Traits/Creator.php');\"",
      "@php -r \"copy('src-override/nesbot/carbon/src/Carbon/Traits/Timestamp.php', 'vendor/nesbot/carbon/src/Carbon/Traits/Timestamp.php');\""
    ],
    "post-update-cmd": [
      "@php -r \"copy('src-override/nesbot/carbon/src/Carbon/Traits/Creator.php', 'vendor/nesbot/carbon/src/Carbon/Traits/Creator.php');\"",
      "@php -r \"copy('src-override/nesbot/carbon/src/Carbon/Traits/Timestamp.php', 'vendor/nesbot/carbon/src/Carbon/Traits/Timestamp.php');\""
    ],
    "phpcs": [
      "./vendor/bin/phpcs"
    ],
    "phpunit": [
      "./vendor/bin/phpunit"
    ],
    "test": [
      "@phpcs",
      "@phpunit"
    ]
  }
}
